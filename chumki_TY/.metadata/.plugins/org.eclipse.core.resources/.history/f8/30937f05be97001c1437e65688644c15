package Practice;

import java.util.LinkedHashSet;

public class Pro {

	public static void main(String[] args) {
		int [] arr= {5,2,8,11,7};
		
	for(int i=0; i<arr.length; i++) {
		for(int j=0; j<arr.length; j++) {
			if(arr[i]<arr[j]) {
				int temp=arr[i];
				arr[i]=arr[j];
				arr[i]=temp;
			}
		}
	}
		int sum=0;
		for(int i=0; i<arr.length; i++) {
		sum=sum+arr[i];
		}
		System.out.println(sum);
	}

}
