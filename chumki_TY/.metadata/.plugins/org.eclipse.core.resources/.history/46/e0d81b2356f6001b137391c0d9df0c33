package StringProgramm;

import java.util.LinkedHashSet;



		
	public class P1 {
		public static void main(String[] args) {
			int [] arr= {8,5,6,9,11,3,8,5};
			int odd=0;
			for(int i=0; i<arr.length; i++) {
				
				
					if(arr[i]%2!=0) {
						if(odd==0) {
							odd=arr[i];
						}
						if(odd<arr[1]) {
							odd=arr[i];
						}
					}
				
			}
			System.out.println("odd no " +odd);
		}
		

		}
		
	//1,3,5,7,9,11
	
			
			