package StringProgramm;

import java.util.LinkedHashSet;



		
	public class P1 {
		public static void main(String[] args) {
			
			String s="chumki";
			char [] ch=s.toCharArray();
			String rev="";
			for(int i=0; i<ch.length;i++) {
				rev=ch[i]+rev;
			}
			System.out.println(rev);
			
			
		}
		

		}
	
			
			